
import variables
@import _variables.scss;
import mixin
@import _mixin.scss;
uso mixin

ul{

    propiedades MOVIL
    @include tablet(){

        propiedades tablet
    }

    li{
        propiedades li
    }
}

mixin boton

@mixin boton($color){

    propiedades;
    background-color: $color
}

usar clase para boton

.botonHeader {

    @include boton (red)
}